{
    "name": "demo/symfony",
    "type": "project",
    "description": "Symfony 8.0 Demo Project",
    "license": "MIT",
    "require": {
        "php": ">=8.5",
        "symfony/symfony": "^8.0",
        "symfony/console": "^8.0",
        "symfony/dotenv": "^8.0",
        "symfony/flex": "^2"
    },
    "require-dev": {
        "nowo-tech/composer-update-helper": "*",
        "phpunit/phpunit": "^11.0"
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "extra": {
        "symfony": {
            "require": "8.0.*"
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}

